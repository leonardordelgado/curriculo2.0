<!--habilidades-->
<ul class="style1 buttons" >
    <li>
        <a href="#habilidades" class="bt" >HABILIDADES</a>
    </li>
</ul>
<hr class="style1">
<div  id="habilidades" class="style1 container default">
    <div class="wrapper">
        <div class="inner">
            <div class="inner">
                <div class="cointainer-card">
                    {{#each post}}
                        <div class="card">
                            <div class="face face1">
                                <div class="content">
                                    <img src="https://raw.githubusercontent.com/Jhonierpc/WebDevelopment/master/CSS%20Card%20Hover%20Effects/img/design_128.png" alt="">
                                    <H3>{{dataValues.title}}</H3>                            
                                </div>
                            </div>
                            <div class="face face2">
                                <div class="content">
                                    <p class="text-cards">
                                        {{dataValues.text}}
                                    </p>
                                </div>
                            </div>
                        </div>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
</div>
<hr class="style1">

<!--sobre-->
<ul class="style1 buttons" >
    <li>
        <a href="#sobre" class="bt DF" >SOBRE</a>
    </li>
</ul>
<hr class="style1">
<div  id="sobre" class="style1 container default">
    <div class="wrapper">
        <div class="inner">
            <div class="inner">
                <div class="infor-sobres">
                    <img src="/pub/img/avata.jpeg" alt="">
                    <h4>Desenvolvedor de soluções ágeis</h4>
                    <h1>LEONARDO RESENDE DELGADO</h1>
                    <p>
                        Desenvolvedor backend e frontend que está em constante evolução para melhor atender as expectativas de um mercado competitivo e dinâmico.
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>
<hr class="style1">

<!--Cursos e certificados-->
<ul class="style1 buttons" >
    <li>
        <a href="#certificados" class="bt DF" >CERTIFICADOS</a>
    </li>
</ul>
<hr class="style1">
<div  id="certificados" class="style1 container default">
    <div class="wrapper">
        <div class="inner-auternativo">
            <div class="inner-auternativo">
                <div class="cointainer-card">
                    <div class="card-cert">
                        <div class="face-cert face1">
                            <div class="content">
                                <img src="/pub/img/logo-alura.svg" class="alura"alt="">
                                <img src="/pub/img/ribbon-banner-alura.svg" class="faixacert" alt="">
                                 <H3></H3>                            
                            </div>
                        </div>
                        <div class="face-cert face3">
                            <div class="content">
                                <div class="title-text-couser">
                                    <p class="text-cards">Formação:</p>
                                    {{#each lerJasonTitle}}
                                        <p class="text-cards">{{title}}</p>
                                    {{/each}}
                                </div>
                                <div class="title-couser">
                                    <p class="text-cards">Cursos:</p>
                                    {{#each lerJasonCourse}}
                                        <p class="text-cards cursos">{{cursos}}</p>
                                    {{/each}}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<hr class="style1">

<!--contatos-->
